<!DOCTYPE html>
<html>
  <head>
    <title>Flight Prices</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        background-color: #f8f8f8;
      }
      h1 {
        font-size: 32px;
        text-align: center;
        color: #1e90ff;
        margin-top: 50px;
        margin-bottom: 30px;
      }
      ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        display: flex;
        flex-direction: column;
        align-items: center;
      }
      li {
        background-color: #fff;
        padding: 20px;
        border-radius: 10px;
        box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.1);
        margin-bottom: 20px;
        width: 80%;
        max-width: 600px;
        display: flex;
        justify-content: space-between;
        align-items: center;
        font-size: 20px;
        color: #1e1e1e;
      }
      li span {
        font-weight: bold;
        font-size: 24px;
        color: #1e90ff;
      }
      input[type="text"] {
        padding: 10px;
        font-size: 18px;
        border-radius: 5px;
        border: none;
        width: 200px;
        margin-bottom: 10px;
      }
      input[type="submit"] {
        background-color: #1e90ff;
        color: #fff;
        border: none;
        border-radius: 5px;
        padding: 10px 20px;
        font-size: 18px;
        cursor: pointer;
      }
      input[type="submit"]:hover {
        background-color: #0d6efd;
      }
    </style>
    </style>
  </head>
  <body>
    
    <% if (data) { %>
    <h1>
      Flight Prices from <%= data.message1 %> to <%= data.message2 %> on
      <%= data.message3 %>
    </h1>
    <ul>
      <% Object.keys(flightPrices).forEach(function(key) { %>
      <li>
        <span><%= key %>:</span> <%= flightPrices[key] %>
      </li>
      <% }); %>
    </ul>
    <% } %>
  </body>
</html>